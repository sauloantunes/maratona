Division
	4/3   = 1
	4/3.0 = 1.333

$ crun 1001
crun ()
{ 
    g++ "$@".cpp -std=c++11;
    ./a.out < in
}

$ cruno 1001
cruno ()
{ 
    g++ "$@".cpp -std=c++11;
    ./a.out < in > out
}

$ uri 1001
uri()
{
    python ./../files/uri.py "$@";
}

$ uris 1001
uris()
{
    python ./../files/uri.py "$@" -s;
}